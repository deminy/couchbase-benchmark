{
    "name": "deminy/couchbase-benchmark",
    "description": "To benchmark Couchbase performance in a non-blocking web server.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "~8.1.0",
        "ext-couchbase": "~3.2.2",
        "ext-swoole": ">=5.0.0",
        "couchbase/couchbase3": "3.2.2",
        "crowdstar/exponential-backoff": "~3.0",
        "crowdstar/reflection": "~2.0",
        "hyperf/config": "~3.0.0",
        "hyperf/di": "~3.0.0",
        "hyperf/framework": "~3.0.0",
        "hyperf/http-server": "~3.0.0",
        "hyperf/logger": "~3.0.0",
        "hyperf/task": "~3.0.0",
        "swoole/ide-helper": "~5.0.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "couchbase/couchbase3",
                "version": "3.2.2",
                "dist": {
                    "url": "https://github.com/couchbase/php-couchbase/archive/v3.2.2.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Crowdstar\\CouchbaseAdapter\\": "contrib/CouchbaseAdapter/",
            "Crowdstar\\OOM\\": "contrib/OOM/"
        }
    },
    "prefer-stable": true,
    "config" : {
        "platform": {
            "php": "8.1.22",
            "ext-couchbase": "3.2.2",
            "ext-swoole": "5.0.3"
        },
        "apcu-autoloader": true,
        "discard-changes": true,
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "post-autoload-dump": [
            "rm -rf runtime/container"
        ]
    }
}
